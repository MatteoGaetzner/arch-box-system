account blaalblab@gmail.com
host smtp.gmail.com
port 587
from blaalblab@gmail.com
user blaalblab@gmail.com
passwordeval "pass blaalblab@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/matteo/.config/msmtp/msmtp.log
# tls_starttls

